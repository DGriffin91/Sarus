
fn main(a, b) -> (e) {
    c, d = stuff(a, b)
    c, d = d, c
    e = if a == b {
        5.0
    } else {
        8.0
    }
    i = 0.0
    while i < 10.0 {
        e = e * 2.0
        i = i + 1.0
    }
}

fn stuff(a, b) -> (c, d) {
    c = a + 1.0
    d = c + b + 10.0
}

